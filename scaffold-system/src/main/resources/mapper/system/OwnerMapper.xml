<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weiho.scaffold.system.mapper.OwnerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.weiho.scaffold.system.entity.Owner">
        <id column="id" property="id"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_del" property="isDel"/>
        <result column="name" property="name" typeHandler="com.weiho.scaffold.mp.handler.NameHandler"/>
        <result column="phone" property="phone" typeHandler="com.weiho.scaffold.mp.handler.PhoneHandler"/>
        <result column="identity_id" property="identityId" typeHandler="com.weiho.scaffold.mp.handler.EncryptHandler"/>
        <result column="email" property="email" typeHandler="com.weiho.scaffold.mp.handler.EncryptHandler"/>
        <result column="password" property="password" typeHandler="com.weiho.scaffold.mp.handler.EncryptHandler"/>
        <result column="sex" property="sex"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        create_time,
        update_time,
        is_del,
        id, name, phone, identity_id, email, password, sex
    </sql>

</mapper>
