<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weiho.scaffold.system.mapper.FeedbackMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.weiho.scaffold.system.entity.Feedback">
        <id column="id" property="id"/>
        <id column="owner_id" property="ownerId"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_del" property="isDel"/>
        <result column="username" property="username"/>
        <result column="type" property="type"/>
        <result column="title" property="title"/>
        <result column="content" property="content"/>
        <result column="feedback_image" property="feedbackImage"/>
        <result column="answer" property="answer"/>
        <result column="answer_time" property="answerTime"/>
        <result column="result" property="result"/>
        <result column="remarks" property="remarks"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        create_time,
        update_time,
        is_del,
        id, owner_id, username, type, title, content, feedback_image, answer, answer_time, result, remarks
    </sql>

</mapper>
